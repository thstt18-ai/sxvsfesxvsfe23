  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development node --import tsx server/index.ts",
    "audit": "cd contracts && slither . --exclude low,medium --fail-on high,critical",
    "storage-check": "forge inspect ArbitrageExecutor storage-layout > out/storage-v1.json",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "auto-build": "cd contracts && npm run auto-build",
    "auto-deploy": "cd contracts && npm run auto-deploy:amoy",
    "auto-deploy:amoy": "cd contracts && npm run auto-deploy:amoy",
    "auto-deploy:polygon": "cd contracts && npm run auto-deploy:polygon",
    "contract:compile": "cd contracts && npm run compile",
    "contract:deploy": "cd contracts && npm run deploy:amoy",
    "test:simulation": "NODE_ENV=test tsx server/testSimulation.ts",
    "setup:testnet": "tsx scripts/setup-testnet.ts",
    "monitor:health": "tsx server/tradingHealthMonitor.ts",
    "ai": "npx hardhat run packages/ai-master/call.js --network amoy",
    "ai:mainnet": "npx hardhat run packages/ai-master/call.js --network polygon",
    "logs": "tail -f packages/ai-master/agent.log"
  },
